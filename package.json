{
  "name": "ae-to-json-cli",
  "version": "1.0.0",
  "description": "This is a command line application to export After Effects files as JSON",
  "main": "index.js",
  "bin": {
    "ae-to-json": "index.js"
  },
  "license": "MIT",
  "author": {
    "name": "Mikko Haapoja",
    "email": "me@mikkoh.com",
    "url": "https://github.com/mikkoh"
  },
  "dependencies": {
    "ae-to-json": "^1.0.0",
    "after-effects": "^0.3.2",
    "app-module-path": "^1.0.6"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0"
  },
  "scripts": {
    "build-after-effects": "npm run copy-after-effects; npm run transpile-after-effects",
    "transpile-after-effects": "babel -d node_modules/after-effects-transpiled/ node_modules/after-effects/",
    "copy-after-effects": "cp -r node_modules/after-effects/ node_modules/after-effects-transpiled/"
  },
  "keywords": [
    "after,effects,json,cli,command,line,terminal,program,export,design,creative"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/jam3/ae-to-json-cli.git"
  },
  "homepage": "https://github.com/jam3/ae-to-json-cli",
  "bugs": {
    "url": "https://github.com/jam3/ae-to-json-cli/issues"
  }
}
